<?xml version="1.0" encoding="UTF-8"?>
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:gwt="urn:import:com.google.gwt.user.client.ui"
             xmlns:my="urn:import:gwtDemo.client.ui">
    <ui:style src="../resource/css/style.css"/>
    <ui:with field="msg" type="gwtDemo.client.resource.i18n.ClientMessages"/>
    <gwt:HTMLPanel>
        <gwt:VerticalPanel>
            <gwt:Label text="{msg.loginTitle}" styleName="{style.caption}"/>
            <gwt:listBox ui:field="languageSelector"/>
        </gwt:VerticalPanel>
    </gwt:HTMLPanel>
</ui:UiBinder>


      
